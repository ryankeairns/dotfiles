	name = Ryan Keairns
[github]
	user = ryankeairns
[alias]
  st = status
  di = diff
  co = checkout
  ci = commit
  br = branch
  sta = stash
[branch]
  autosetupmerge = true
  autosetuprebase = always
[color]
  branch = auto
  diff = auto
  status = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[core]
	excludesfile = ~/.gitignore